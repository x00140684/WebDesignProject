# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
GET     /about                           controllers.HomeController.about
GET     /basket                           controllers.HomeController.basket
GET     /cars                           controllers.HomeController.cars
GET     /contact                           controllers.HomeController.contact
GET     /getinvolved                           controllers.HomeController.getinvolved
GET     /login                           controllers.HomeController.login
GET     /parts                           controllers.HomeController.parts
GET     /register                           controllers.HomeController.register

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET /login                      controllers.LoginController.login
POST /loginSubmit               controllers.LoginController.loginSubmit
GET /logut                      controllers.LoginController.logout